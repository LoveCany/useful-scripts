{
   "background": {
      "page": "background.html"
   },
   "browser_action": {
      "default_icon": "img/icon.png",
      "default_popup": "popup.html"
   },
   "description": "Web media sniffing tool",
   "icons": {
      "40": "img/icon.png",
      "48": "img/icon.png",
      "128": "img/icon128.png"
   },
   "manifest_version": 2,
   "minimum_chrome_version": "30",
   "name": "Cat Scratch [English Version]",
   "options_page": "options.html",
   "permissions": [ "tabs", "webRequest", "webRequestBlocking", "*://*/*", "<all_urls>"],
   "version": "1.0.12"
}
